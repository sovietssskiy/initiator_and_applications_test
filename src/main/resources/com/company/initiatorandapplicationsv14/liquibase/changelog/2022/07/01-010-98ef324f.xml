<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="initiator-and-applications-v1-4">
        <addColumn tableName="APPLICATION">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="2" author="initiator-and-applications-v1-4">
        <addColumn tableName="DEPARTAMENT">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="3" author="initiator-and-applications-v1-4">
        <addColumn tableName="INITIATOR">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="4" author="initiator-and-applications-v1-4">
        <addColumn tableName="POSITION_">
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
        </addColumn>
    </changeSet>
    <changeSet id="5" author="initiator-and-applications-v1-4">
        <addColumn tableName="APPLICATION">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="APPLICATION"/>
    </changeSet>
    <changeSet id="6" author="initiator-and-applications-v1-4">
        <addColumn tableName="DEPARTAMENT">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="DEPARTAMENT"/>
    </changeSet>
    <changeSet id="7" author="initiator-and-applications-v1-4">
        <addColumn tableName="INITIATOR">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="INITIATOR"/>
    </changeSet>
    <changeSet id="8" author="initiator-and-applications-v1-4">
        <addColumn tableName="POSITION_">
            <column name="VERSION" type="INT"/>
        </addColumn>

        <addNotNullConstraint columnName="VERSION" defaultNullValue="0" tableName="POSITION_"/>
    </changeSet>
</databaseChangeLog>